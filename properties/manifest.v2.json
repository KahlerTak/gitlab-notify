{
  "name": "Gitlab Notifications",
  "description": "An extension for additional notifications on Gitlab",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [ "tabs", "notifications", "alarms", "scripting", "storage" ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "extension-icon.png"
  }
}
