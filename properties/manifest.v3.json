{
  "name": "Gitlab Notifications",
  "description": "An extension for additional notifications on Gitlab",
  "manifest_version": 3,
  "permissions": [ "notifications", "alarms", "storage" ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "extension-icon-128.png",
      "32": "extension-icon-128.png",
      "48": "extension-icon-128.png",
      "128": "extension-icon-128.png"
    },
    "default_title": "Gitlab Notifications"
  },
  "options_page": "options.html"
}
